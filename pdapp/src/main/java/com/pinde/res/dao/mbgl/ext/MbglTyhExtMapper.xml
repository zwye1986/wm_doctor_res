<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pinde.res.dao.mbgl.ext.MbglTyhExtMapper">
    <resultMap id="BaseResultMap" type="hashmap" >
        <id column="ID" property="id" jdbcType="DECIMAL" />
        <id column="YHM" property="yhm" jdbcType="VARCHAR" />
        <result column="MM" property="mm" jdbcType="VARCHAR" />
        <result column="TYPE" property="type" jdbcType="DECIMAL" />
        <result column="HZID" property="hzid" jdbcType="DECIMAL" />
        <result column="ZSXM" property="zsxm" jdbcType="VARCHAR" />
        <result column="XB" property="xb" jdbcType="DECIMAL" />
        <result column="ZC" property="zc" jdbcType="VARCHAR" />
        <result column="SFZ" property="sfz" jdbcType="VARCHAR" />
        <result column="SJ" property="sj" jdbcType="VARCHAR" />
        <result column="YX" property="yx" jdbcType="VARCHAR" />
        <result column="BZ" property="bz" jdbcType="VARCHAR" />
        <result column="SFTY" property="sfty" jdbcType="DECIMAL" />
        <result column="ring_id" property="ringId" jdbcType="VARCHAR" />
        <result column="ring_pass" property="ringPass" jdbcType="VARCHAR" />
    </resultMap>
    <update id="updateRingInfo">
        update t_yh set ring_id=#{yhm},ring_pass=#{s}
        where yhm=#{yhm}
    </update>
    <select id="userList" resultMap="BaseResultMap">
        SELECT * FROM t_yh where SFTY = 0
    </select>
    <select id="readOtherUsers" resultMap="BaseResultMap">
        SELECT * FROM t_yh where SFTY = 0 and yhm!=#{userCode}
        and ring_id is not null
    </select>
    <select id="selectByCode" resultMap="BaseResultMap">
        SELECT * FROM t_yh where SFTY = 0 and yhm=#{userCode}
    </select>

</mapper>
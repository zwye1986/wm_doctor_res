<?xml version="1.0" encoding="UTF-8"?>
<jspForm>	
	<!-- 初始审查工作表-->
	<!-- categroy id
	1：药物试验
	2：临床科研
	3：医疗器械
	{0}:表单所属机构(开关控制)
	{1}:项目类别
	{2}:版本号
 -->
	<jsp ver="1.0" path="irb/form/{0}/worksheet/retrial_{1}_{2}"></jsp>
	<product>
		<page categroy="yw" ver="1.0">
		<!-- 审查要素 -->
			<item name="reviewElement1" remark="修正案符合伦理委员会的要求" multiple="N">
				<code value="1">是</code>
				<code value="0">否</code>
			</item>
			<item name="reviewElement2" remark="认可申请人对伦理委员会建议所作的说明" multiple="N">
				<code value="1">是</code>
				<code value="0">否</code>
			</item>
		
		<!-- 审查意见 -->
			<item name="suggest" remark="建议" multiple="N"></item>
			<item name="decision" remark="决定" multiple="N"></item>
			<item name="submitMeetingReview" remark="提交会议审查" multiple="N"></item>
			<item name="preCommitteeName" remark="主审委员签名" multiple="N"></item>
			<item name="signDate" remark="主审委员签字日期" multiple="N"></item>
		</page>
	</product>
</jspForm>


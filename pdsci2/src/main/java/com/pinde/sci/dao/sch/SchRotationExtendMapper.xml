<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pinde.sci.dao.sch.SchRotationExtendMapper" >
  <resultMap id="SysDictMap" type="com.pinde.sci.model.mo.SysDict" >
    <id column="DICT_FLOW" property="dictFlow" jdbcType="VARCHAR" />
    <result column="DICT_TYPE_ID" property="dictTypeId" jdbcType="VARCHAR" />
    <result column="DICT_TYPE_NAME" property="dictTypeName" jdbcType="VARCHAR" />
    <result column="DICT_ID" property="dictId" jdbcType="VARCHAR" />
    <result column="DICT_NAME" property="dictName" jdbcType="VARCHAR" />
    <result column="DICT_DESC" property="dictDesc" jdbcType="VARCHAR" />
    <result column="ORDINAL" property="ordinal" jdbcType="DECIMAL" />
    <result column="RECORD_STATUS" property="recordStatus" jdbcType="VARCHAR" />
    <result column="CREATE_TIME" property="createTime" jdbcType="VARCHAR" />
    <result column="CREATE_USER_FLOW" property="createUserFlow" jdbcType="VARCHAR" />
    <result column="MODIFY_TIME" property="modifyTime" jdbcType="VARCHAR" />
    <result column="MODIFY_USER_FLOW" property="modifyUserFlow" jdbcType="VARCHAR" />
    <result column="DICT_ISSYS" property="dictIssys" jdbcType="VARCHAR" />
    <result column="ORG_NAME" property="orgName" jdbcType="VARCHAR" />
    <result column="ORG_FLOW" property="orgFlow" jdbcType="VARCHAR" />
    <result column="DICT_NAME_EN" property="dictNameEn" jdbcType="VARCHAR" />
  </resultMap>

  <select id="selectDictDoctorTrainingSpe" resultMap="SysDictMap">
        SELECT  DICT_FLOW, DICT_TYPE_ID, DICT_TYPE_NAME, DICT_ID, DICT_NAME, DICT_DESC, ORDINAL,
    RECORD_STATUS, CREATE_TIME, CREATE_USER_FLOW, MODIFY_TIME, MODIFY_USER_FLOW, DICT_ISSYS,
    ORG_NAME, ORG_FLOW, DICT_NAME_EN
    FROM SYS_DICT  WHERE DICT_TYPE_ID = 'DoctorTrainingSpe'
    AND RECORD_STATUS = 'Y'
    </select>

  <select id="selectDictStandardDept" resultMap="SysDictMap">
        SELECT  DICT_FLOW, DICT_TYPE_ID, DICT_TYPE_NAME, DICT_ID, DICT_NAME, DICT_DESC, ORDINAL,
    RECORD_STATUS, CREATE_TIME, CREATE_USER_FLOW, MODIFY_TIME, MODIFY_USER_FLOW, DICT_ISSYS,
    ORG_NAME, ORG_FLOW, DICT_NAME_EN
    FROM SYS_DICT  WHERE DICT_TYPE_ID like 'StandardDept%'
    AND RECORD_STATUS = 'Y'
    </select>
</mapper>
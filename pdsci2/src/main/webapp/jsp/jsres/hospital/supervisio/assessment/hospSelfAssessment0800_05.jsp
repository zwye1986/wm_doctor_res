        </tr>
        <tr>
            <td style="text-align: center;">5</td>
            <td> 测量中注意事项</td>
            <td style="text-align: center;">5</td>
            <td style="text-align: center;">
                <c:if test="${type eq 'Y'}">
                    <input onchange="saveScore(this,10);" itemId="11" name="first" class="input" type="text" value="${scoreMap['11']}"  style="width: 80%;height:20px; text-align: center"/>
                </c:if>
                <c:if test="${type ne 'Y'}">
                    ${scoreMap['11']}
